{
  "version": "2",
  "templates": [
    {
      "type": 3,
      "title": "App Magento",
      "description": "Diversify your customer base. Weâ€™ll tailor their commerce experience.",
      "note": "Adobe Commerce uses AI and advanced data sharing capabilities to create end-to-end personalized B2C and B2B commerce experiences from a single platform that is flexible, extensible, and scalable.",
      "categories": [
        "php",
        "ecommerce",
        "adobe",
        "magento"
      ],
      "platform": "linux",
      "logo": "https://raw.githubusercontent.com/aartintelligent/ecosystem/master/portainer/stack/app-magento/icon.svg",
      "repository": {
        "url": "https://github.com/aartintelligent/ecosystem",
        "stackfile": "portainer/stack/app-magento/docker-compose.yml"
      },
      "env": [
        {
          "name": "DOMAIN",
          "label": "Local domain"
        },
        {
          "name": "MAGENTO_VERSION",
          "label": "Symfony version",
          "select": [
            {
              "text": "Latest",
              "value": "latest",
              "default": true
            },
            {
              "text": "2.4",
              "value": "2.4"
            }
          ]
        }
      ],
      "volumes": [
        {
          "container": "/var/www"
        }
      ]
    },
    {
      "type": 3,
      "title": "App Symfony",
      "description": "Symfony is a set of reusable PHP components ... and a PHP framework for web projects",
      "note": "The standard foundation on which the best PHP applications are built. Choose any of the 50 stand-alone components available for your own applications. ",
      "categories": [
        "php",
        "framework",
        "symfony"
      ],
      "platform": "linux",
      "logo": "https://raw.githubusercontent.com/aartintelligent/ecosystem/master/portainer/stack/app-symfony/icon.svg",
      "repository": {
        "url": "https://github.com/aartintelligent/ecosystem",
        "stackfile": "portainer/stack/app-symfony/docker-compose.yml"
      },
      "env": [
        {
          "name": "DOMAIN",
          "label": "Local domain"
        },
        {
          "name": "SYMFONY_VERSION",
          "label": "Symfony version",
          "select": [
            {
              "text": "Latest",
              "value": "latest",
              "default": true
            },
            {
              "text": "6.2",
              "value": "6.2"
            }
          ]
        },
        {
          "name": "MYSQL_VERSION",
          "label": "Mysql version",
          "select": [
            {
              "text": "8.0",
              "value": "8.0",
              "default": true
            },
            {
              "text": "5.7",
              "value": "5.7"
            }
          ]
        },
        {
          "name": "DATABASE_NAME",
          "label": "Database name",
          "default": "magento",
          "preset": true
        },
        {
          "name": "DATABASE_PASSWORD",
          "label": "Database password",
          "default": "nopassword",
          "preset": true
        },
        {
          "name": "RABBITMQ_VERSION",
          "label": "Mysql version",
          "select": [
            {
              "text": "3.11",
              "value": "3.11",
              "default": true
            }
          ]
        },
        {
          "name": "RABBITMQ_USERNAME",
          "label": "Rabbitmq username",
          "default": "root",
          "preset": true
        },
        {
          "name": "RABBITMQ_PASSWORD",
          "label": "Rabbitmq password",
          "default": "nopassword",
          "preset": true
        }
      ],
      "volumes": [
        {
          "container": "/var/www"
        }
      ]
    }
  ]
}
